###! configure enviroment !###
export GOROOT=/usr/local/opt/go/libexec/
export GOPATH=$HOME/go
export PATH="${PATH}:${HOME}/local/bin:$GOPATH/bin:$GOROOT/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin:$HOME/.local/bin/:$HOME/.local/jdk1.8.0_102/bin/:/usr/local/opt/coreutils/libexec/gnubin:$HOME/bin"
export LANG=en_US.UTF-8
export EDITOR='vim'
export SHELL='zsh'
export PATH="$HOME/.rbenv/shims:/usr/local/bin:$HOME/.linuxbrew/bin:/Applications/CopyQ.app/Contents/MacOS/:$PATH"
eval $(gdircolors -b ~/.dirc)
export ZSH_ENV_LOADED="1"
eval "$(anyenv init - --no-rehash)"

